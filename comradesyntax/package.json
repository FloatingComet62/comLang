{
  "name": "comradesyntax",
  "displayName": "comradeSyntax",
  "description": "Syntax Highlight for Comrade Programming Language",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.78.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
