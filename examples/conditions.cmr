include std->io

fun main(_argc -> i32, _argv -> str[]) => u8 {
    io->out("Enter age: ")
    const input = io->in(i32)
    if input >= 18 {
        io->out("Here is your beer\n")
    } else if input >= 13 {
        io->out("Here is your pepsi\n")
    } else {
        io->out("Here is your orange juice\n")
    }
    return 0
}