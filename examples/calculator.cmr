include std->io

fun main(_argc -> i32, _argv -> str[]) => u8 {
  while true {
    const operation = io->in<str>() # this is a comment
    let num1 = io->in(i32)
    let num2 = io->in(i32)
    io->out(match operation.lowercase() {
      "a" => num1 + num2
      "s" => num1 - num2
      "m" => num1 * num2
      "d" => num1 / num2
      
      default => "Unknown Operation"
    })
  }
  return 0
}
